{
    "manifest_version": 3,
    "name": "DuoTracker",
    "version": "1.0",
    "description": "Identify who doesn't follow you back on Duolingo",
    "permissions": [
        "storage",
        "cookies"
    ],
    "host_permissions": [
        "https://www.duolingo.com/*"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    },
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}